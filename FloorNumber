import java.util.*;
class Main {
	public static int solve(int A) {
        double ans= A/200d;
        int x = (int)ans;
        double z = -1;
        if(ans == x){
            return x;
        }
        else if(ans>=0){
            return x;
        }
        else{
           z = ans+z;
           return (int)z; 
        }
    }
	public static void main(String args[]) {
		// Your code goes here
		Scanner sc = new Scanner(System.in);
		int A = sc.nextInt();
		int answer = solve(A);
		System.out.println(answer);
		
}
}
